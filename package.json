{
	"name": "click-clack",
	"version": "0.1.1",
	"description": "Make some noise when typing in Obsidian.md!",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ./src"
	},
	"keywords": [
		"typewriter",
		"mechanical-keyboard",
		"obsidian",
		"obsidian-plugin",
		"obsidian-md",
		"obsidian-community"
	],
	"author": "Acylation",
	"license": "MIT",
	"devDependencies": {
		"@types/howler": "^2.2.8",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@zip.js/zip.js": "^2.7.40",
		"howler": "^2.2.3",
		"i18next": "^23.10.1"
	}
}